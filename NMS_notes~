DMS to DD: 
var = hour+(((minute*60)+(second))/3600);

var DMS = function convert_DMS() {
    var lat = 40+(((44*60)+(24))/3600);
    var long = 73+(((59*60)+(22))/3600);    
    console.log('Lat: ' + lat)
    console.log('Long: ' + long)
        
}
DMS();

